{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "StoragePath": "/tmp/DockerMount",
    "FileNameSettings": {
      "SeparateYearMonthDay": false,
      "SeparateTimeFromDate": true,
      "Separator": "_",
      "FileNameOrder": [
        "DAY",
        "TIME",
        "ORGFN"
      ]
    },
    "DirectoryFormat": {
      "Directory": "YEAR",
      "SubDirectory":{
        "Directory": "MONTH",
        "Subdirectories": null
      }
    }
  }
}
