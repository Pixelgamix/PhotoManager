{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "localhost",
  "Database": {
    "StoragePath": "/tmp/DockerMount",
    "FileNameSettings": {
      "SeparateYearMonthDay": false,
      "SeparateTimeFromDate": true,
      "Separator": "_",
      "FileNameOrder": [
        "DAY",
        "TIME",
        "ORGFN"
      ]
    },
    "DirectoryFormat": {
      "Directory": "YEAR",
      "SubDirectory":{
        "Directory": "MONTH",
        "Subdirectories": null 
      }
    }
  }
}
